<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="3.0">
  
    <servlet>
        <servlet-name>SumServlet</servlet-name>
        <servlet-class>com.servletExample.Sum</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SumServlet</servlet-name>
        <url-pattern>/sum</url-pattern>
    </servlet-mapping>

    
    <servlet>
        <servlet-name>totalServlet</servlet-name>
        <servlet-class>com.servletExample.TotalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>totalServlet</servlet-name>
        <url-pattern>/total</url-pattern>
    </servlet-mapping>
</web-app>
